    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "source_encrypt",
            "justMyCode": true,
            "args" : ["runtime", "-k" ,"${workspaceFolder}/key", "out"],
            "cwd" : "${workspaceFolder}"
        },
        {
            "name": "Python: encrypt",
            "type": "python",
            "request": "launch",
            "module": "source_encrypt",
            "justMyCode": true,
            "args" : ["obfuscate", "-k" ,"${workspaceFolder}/key", "-s", "${workspaceFolder}/test_input.py", "-d", "${workspaceFolder}/test_output.py"],
            "cwd" : "${workspaceFolder}"
        },
        {
            "name": "Python: run_encrypt",
            "type": "python",
            "request": "launch",
            "program": "test_output.py",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/outfolder/win_amd64/cp39-cp39"
            },
            "cwd" : "${workspaceFolder}"
        }
    ]
}